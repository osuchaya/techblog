<!--has to have navbar displaying home button, dashboard button -->

<section id="blogcontainer">
<div class="blog-container">
{{#each blogposts as |post| }}

<a href="/blog/{{post.id}}"><h1>{{post.title}}   {{post.user.user_name}}   {{post.date_created}}</h1></a>
<p>{{post.content}}</p>
{{/each}}
</div>
</section>
